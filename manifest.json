{
  "applications": {
    "gecko": {
      "id": "note-tab@aminomancer"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "note-tab@aminomancer"
    }
  },
  "icons": {
    "96": "icons/favicon.png"
  },
  "manifest_version": 2,
  "name": "Tab Notes",
  "version": "5.7.9",
  "description": "Make new tab page a notes input page",
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "chrome_settings_overrides": {
    "homepage": "newtab.html"
  },

  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/favicon.png",
    "default_title": "Tab Notes",
    "default_popup": "newtab.html"
  }
}